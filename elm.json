{
    "type": "package",
    "name": "lattyware/elm-material",
    "summary": "An elm wrapper for a library of web components that follows Material Design.",
    "license": "MIT",
    "version": "1.0.0",
    "exposed-modules": [
        "Material.Button",
        "Material.Chips",
        "Material.Fab",
        "Material.IconButton",
        "Material.ListView",
        "Material.Menu",
        "Material.Select",
        "Material.Switch",
        "Material.TextField"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "source-directories": [
        "src"
    ],
    "dependencies": {
        "elm/core": "1.0.2 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.1.3 <= v < 1.1.3"
    },
    "test-dependencies": {}
}
